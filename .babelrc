{
  "presets": [
    ["env", {
      modules: false,
      useBuiltIns: "usage",
      targets: [
          "> 1%",
          "last 2 versions",
          "not ie <= 9"
      ]
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@constants": "./src/constants.js",
        "@assets": "./assets",
        "@tests-mocks": "./mocks",
        "@actions": "./src/store/actions",
        "@utils": "./src/utils",
        "@optionComponents": "./src/components/Widget/components/Conversation/components/Messages/components/OptionMessage/components",
        "@messagesComponents": "./src/components/Widget/components/Conversation/components/Messages/components"
      }
    }]
  ]
}
